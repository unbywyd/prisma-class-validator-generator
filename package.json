{
  "name": "prisma-class-dto-generator",
  "version": "5.0.0",
  "description": "Prisma 2+ generator to emit TypeScript models of your database with class validator and class transformer",
  "bin": {
    "prisma-class-dto-generator": "lib/generator.js"
  },
  "main": "lib/generator.js",
  "scripts": {
    "start": "tsc && npx prisma generate",
    "package:publish": "./package.sh && cd package && npm publish"
  },
  "author": {
    "name": "Omar Dulaimi",
    "url": "https://github.com/omar-dulaimi"
  },
  "contributors": [
    {
      "name": "Artyom Gorlovetskiy",
      "url": "https://github.com/unbywyd"
    }
  ],
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "class-validator",
    "class-transformer",
    "prisma-generator",
    "prisma-class-dto-generator"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "@prisma/generator-helper": "^5.12.1",
    "@prisma/internals": "^5.12.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.1",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.8",
    "ts-morph": "^22.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.31",
    "@types/pluralize": "0.0.33",
    "@types/prettier": "^2.7.3",
    "prisma": "^5.12.1",
    "typescript": "^5.4.5"
  },
  "bugs": {
    "url": "https://github.com/omar-dulaimi/prisma-class-dto-generator/issues"
  },
  "homepage": "https://github.com/omar-dulaimi/prisma-class-dto-generator#readme",
  "private": true
